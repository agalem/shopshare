{
    "name": "fabpot/silex-skeleton",
    "description": "A pre-configured skeleton for the Silex microframework",
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=5.5.9",
        "silex/silex": "^2.2",
        "twig/twig": "^1.35",
        "symfony/asset": "^3.4",
        "symfony/twig-bridge": "^3.4",
        "twbs/bootstrap": "^4.1",
        "silex/web-profiler": "^2.0",
        "symfony/browser-kit": "^3.4",
        "symfony/class-loader": "^3.4",
        "symfony/config": "^3.4",
        "symfony/console": "^3.4",
        "symfony/css-selector": "^3.4",
        "symfony/debug": "^3.4",
        "symfony/finder": "^3.4",
        "symfony/form": "^3.4",
        "symfony/monolog-bridge": "^3.4",
        "symfony/security": "^3.4",
        "symfony/process": "^3.4",
        "symfony/translation": "^3.4",
        "symfony/validator": "^3.4"
    },
    "autoload": {
        "psr-0": { "": "src/" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0.x-dev"
        }
    },
    "scripts": {
        "run": [
            "echo 'Started web server on http://localhost:8888'",
            "php -S localhost:8888 -t web"
        ]
    }
}